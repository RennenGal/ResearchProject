{
  "mcpServers": {
    "uniprot-server": {
      "command": "uv",
      "args": ["--directory", "/path/to/uniprot-mcp-server", "run", "uniprot-mcp-server"],
      "env": {
        "PYTHONPATH": "/path/to/uniprot-mcp-server"
      },
      "disabled": true,
      "autoApprove": [
        "get_protein_info",
        "get_batch_protein_info"
      ]
    }
  }
}